<Window x:Class="FestpunktDB.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:uc="clr-namespace:FestpunktDB.GUI"
        mc:Ignorable="d"
        Title="Datenbank Festpunktfeld Team BahnGeodaten, RB OST" Height="700" Width="1300">
    <DockPanel>
        <TabControl x:Name="MainTabControl" Background="{x:Null}" SelectionChanged="MainTabControl_SelectionChanged">
            <TabItem x:Name="ViewTabItem" Header="Ansicht" HorizontalAlignment="Center" Height="20" VerticalAlignment="Center" Width="54">
                <!--MouseLeftButtonUp="ViewTabItem_MouseLeftButtonUp "-->
                <Grid Background="#FFE5E5E5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="277*"/>
                        <ColumnDefinition Width="718*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto">
                            <ColumnDefinition.Style>
                                <Style TargetType="ColumnDefinition">
                                    <Setter Property="MinWidth" Value="0" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding ElementName=FilterExpander, Path=IsExpanded}" Value="True">
                                            <Setter Property="MinWidth" Value="290" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </ColumnDefinition.Style>
                        </ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid x:Name="Tables" Margin="5" Grid.RowSpan="2" Grid.ColumnSpan="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="56*" MinHeight="300"/>
                            <RowDefinition Height="11*"/>
                            <RowDefinition Height="11*"/>
                            <RowDefinition Height="11*"/>
                            <RowDefinition Height="11*"/>
                        </Grid.RowDefinitions>
                        <Grid Grid.Row="0" x:Name="PpTablePanel">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="33*"/>
                                <ColumnDefinition Width="240*"/>
                                <ColumnDefinition Width="56*"/>
                                <ColumnDefinition Width="892*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="20"/>
                                <RowDefinition/>
                                <RowDefinition Height="22"/>
                            </Grid.RowDefinitions>
                            <DockPanel Grid.ColumnSpan="4">
                                <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" Text="Tabelle PP" FontSize="13"/>
                                <Button x:Name="RefreshButton" DockPanel.Dock="Right" Width="20" Background="{x:Null}" Click="RefreshButton_Click" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <Image Source="/resources/refresh.png"></Image>
                                </Button>
                                <DockPanel HorizontalAlignment="Right" VerticalAlignment="Stretch" DockPanel.Dock="Right" Margin="0,0,25,0">
                                    <TextBox x:Name="QtEntriesPerPageInput" DockPanel.Dock="Left" VerticalAlignment="Center" Text="100" HorizontalAlignment="Left" MinWidth="20"/>
                                    <TextBlock Text=" Einträge pro Seite" VerticalAlignment="Center" Margin="0,0,10,0" HorizontalAlignment="Stretch"/>
                                    <Button x:Name="QtEntriesPerPageConfirmButton" DockPanel.Dock="Right" Content="OK" VerticalAlignment="Center" HorizontalAlignment="Center" Height="18" Click="QtEntriesPerPageConfirmButton_Click"/>
                                </DockPanel>
                            </DockPanel>
                            <DataGrid Grid.Row="1" x:Name="PpTable" AutoGenerateColumns="True"
                                      AutoGeneratingColumn="PpTable_OnAutoGeneratingColumn"
                                      SelectionChanged="PpTable_SelectionChanged" ColumnWidth="*" SelectionMode="Extended" IsReadOnly="True" Grid.ColumnSpan="4" Margin="0,0,0,22" Grid.RowSpan="2"/>
                            <Border Grid.Row="2" BorderThickness="0,0,0,1" BorderBrush="Gray" Grid.ColumnSpan="4">
                                <DockPanel DockPanel.Dock="Bottom" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" LastChildFill="False">
                                    <TextBlock x:Name="EntryCountTextBlock" FontStyle="Italic" VerticalAlignment="Center"/>
                                    <TextBlock Text=" Einträge" VerticalAlignment="Center" FontStyle="Italic"/>
                                    <Button x:Name="LastPageRightButton" Content="&gt;|" FontSize="15"
                                            FontFamily="Bodoni MT Black" Margin="5,0,0,0" Click="LastPageRightButton_Click" DockPanel.Dock="Right"/>
                                    <Button x:Name="MorePageRightButton" Content="&gt;&gt;" FontSize="15" ToolTip="nächste 5 Seiten"
                                            FontFamily="Bodoni MT Black" Margin="5,0,0,0" Click="MorePageRightButton_Click" DockPanel.Dock="Right"/>
                                    <Button x:Name="PageRightButton" Content="&gt;" FontSize="15"
                                            FontFamily="Bodoni MT Black" Margin="5,0,0,0" Click="PageRightButton_Click" DockPanel.Dock="Right"/>
                                    <TextBlock x:Name="PageCountTextBlock" VerticalAlignment="Center" DockPanel.Dock="Right"></TextBlock>
                                    <TextBlock Text=" von " VerticalAlignment="Center" DockPanel.Dock="Right"></TextBlock>
                                    <ComboBox x:Name="PageNumberComboBox" IsEditable="True" MinWidth="30"
                                              TextBoxBase.TextChanged="PageNumberComboBox_TextChanged" SelectedIndex="0" DockPanel.Dock="Right"/>
                                    <TextBlock Text="Seite " FontWeight="Normal" HorizontalAlignment="Stretch" VerticalAlignment="Center" DockPanel.Dock="Right"></TextBlock>
                                    <Button x:Name="PageLeftButton" Content="&lt;" FontSize="15"
                                            FontFamily="Bodoni MT Black" Margin="0,0,5,0" Click="PageLeftButton_Click" DockPanel.Dock="Right">
                                        <Button.Style>
                                            <Style TargetType="Button">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger
                                                        Binding="{Binding ElementName=PageNumberComboBox, Path=Text}"
                                                        Value="1">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Button.Style>
                                    </Button>
                                    <Button x:Name="MorePageLeftButton" Content="&lt;&lt;" FontSize="15" ToolTip="vorherige 5 Seiten"
                                            FontFamily="Bodoni MT Black" Margin="0,0,5,0" Click="MorePageLeftButton_Click" DockPanel.Dock="Right">
                                        <Button.Style>
                                            <Style TargetType="Button">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger
                                                        Binding="{Binding ElementName=PageNumberComboBox, Path=Text}"
                                                        Value="1">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Button.Style>
                                    </Button>
                                    <Button x:Name="LastPageLeftButton" Content="|&lt;" FontSize="15"
                                            FontFamily="Bodoni MT Black" Margin="0,0,5,0" Click="LastPageLeftButton_Click" DockPanel.Dock="Right" FontWeight="Normal" FontStyle="Normal">
                                        <Button.Style>
                                            <Style TargetType="Button">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger
                                                        Binding="{Binding ElementName=PageNumberComboBox, Path=Text}"
                                                        Value="1">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </Button.Style>
                                    </Button>
                                </DockPanel>
                            </Border>
                        </Grid>
                        <DockPanel x:Name="PlTablePanel" Grid.Row="3" Margin="0,3,0,0">
                            <TextBlock TextWrapping="Wrap" DockPanel.Dock="Top" FontSize="13"><Run Language="de-de" Text="Tabelle PL"/></TextBlock>
                            <DataGrid x:Name="PlTable" DockPanel.Dock="Top" ColumnWidth="*" AutoGeneratingColumn="PlTable_AutoGeneratingColumn" SelectionChanged="PlTable_SelectionChanged" IsReadOnly="True"/>
                        </DockPanel>
                        <DockPanel x:Name="PhTablePanel" Grid.Row="1" Margin="0,3,0,0">
                            <TextBlock TextWrapping="Wrap" DockPanel.Dock="Top" FontSize="13"><Run Text="Tabelle PH"/></TextBlock>
                            <DataGrid x:Name="PhTable" DockPanel.Dock="Top" ColumnWidth="*" AutoGeneratingColumn="PhTable_AutoGeneratingColumn" SelectionChanged="PhTable_SelectionChanged" IsReadOnly="True"/>
                        </DockPanel>
                        <DockPanel x:Name="PkTablePanel" Grid.Row="2" Margin="0,3,0,0">
                            <TextBlock TextWrapping="Wrap" DockPanel.Dock="Top"><Run Text="Tabelle PK" FontSize="13"/></TextBlock>
                            <DataGrid x:Name="PkTable" DockPanel.Dock="Top" ColumnWidth="*" AutoGeneratingColumn="PkTable_AutoGeneratingColumn" SelectionChanged="PkTable_SelectionChanged" IsReadOnly="True"/>
                        </DockPanel>
                        <DockPanel x:Name="PsTablePanel" Grid.Row="4" Margin="0,3,0,0">
                            <TextBlock TextWrapping="Wrap" DockPanel.Dock="Top"><Run Text="Tabelle PS" FontSize="13"/></TextBlock>
                            <DataGrid x:Name="PsTable" DockPanel.Dock="Top" ColumnWidth="*" AutoGeneratingColumn="PsTable_AutoGeneratingColumn" SelectionChanged="PsTable_SelectionChanged" IsReadOnly="True"/>
                        </DockPanel>
                    </Grid>
                    <Grid x:Name="DataSheetDisplay" Grid.Column="3" Grid.Row="1" Margin="5,5,5,5" 
                          ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Auto">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Image x:Name="JpgDisplay" ScrollViewer.VerticalScrollBarVisibility="Disabled"/>
                        <WebBrowser x:Name="PdfDisplay" Grid.Row="0" MinWidth="300" ScrollViewer.VerticalScrollBarVisibility="Disabled" Visibility="Collapsed"/>
                        <Grid x:Name="TrafficLight" Grid.Row="1" HorizontalAlignment="Right" Visibility="Collapsed">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="JpgIndicator" HorizontalAlignment="Center" VerticalAlignment="Center" Content="JPG" BorderBrush="Black" BorderThickness="1" 
                                   Background="Red" ToolTip="Klicken Sie, um eine fehlende Skizzendatei hinzuzufügen" MouseUp="JpgIndicator_MouseUp" Grid.RowSpan="3" Width="33" Margin="0.5"/>
                            <Label x:Name="PdfIndicator" HorizontalAlignment="Center" VerticalAlignment="Center" Content="PDF" BorderBrush="Black" BorderThickness="1"
                                   Grid.Column="1" Margin="0.5"
                                   Background="Red" ToolTip="Klicken Sie, um eine fehlende Skizzendatei hinzuzufügen" MouseUp="PdfIndicator_MouseUp" Grid.RowSpan="2" Width="33"/>
                            <Label x:Name="PptIndicator" HorizontalAlignment="Center" VerticalAlignment="Center" Content="PPT" BorderBrush="Black" BorderThickness="1"
                                   Grid.Column="2" Margin="0.5"
                                   Background="Red" ToolTip="Klicken Sie, um eine fehlende Skizzendatei hinzuzufügen" MouseUp="PptIndicator_MouseUp" Width="33"/>
                        </Grid>
                    </Grid>
                    <Expander x:Name="FilterExpander" Header="Filter" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="3" AutomationProperties.Name="Filter" HorizontalContentAlignment="Right">
                        <UniformGrid x:Name="FilterGrid" Background="#FFC6C1C1" VerticalAlignment="Top" HorizontalAlignment="Right" MinWidth="280" Rows="6" >
                            <DockPanel Margin="3,0,3,0">
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="3,0,0,0">
                                    <TextBlock TextWrapping="Wrap" Text="PAD:" Margin="0,0,7,0" FontWeight="Normal"/>
                                    <TextBox x:Name="PadInput" TextWrapping="Wrap" DockPanel.Dock="Left" MinWidth="50" TextChanged="PadInput_TextChanged"/>
                                    <Image ToolTip="Die Tabelle wird nach mindestens 4 Ziffern durch PAD gefiltert." DockPanel.Dock="Right" HorizontalAlignment="Center" Source="/resources/info.png" Stretch="Fill" Width="12" VerticalAlignment="Top"/>
                                </DockPanel>
                                <DockPanel DockPanel.Dock="Right" HorizontalAlignment="Right" VerticalAlignment="Center">
                                    <TextBlock TextWrapping="Wrap" DockPanel.Dock="Left" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,0,14,0"><Run Text="Punktart:"/></TextBlock>
                                    <ComboBox x:Name="StatusSelection" DockPanel.Dock="Left" IsDropDownOpen="False" Width="50" HorizontalAlignment="Left" VerticalAlignment="Center"  DropDownClosed="StatusSelection_DropDownClosed">
                                        <ComboBoxItem Content="PS0"/>
                                        <ComboBoxItem Content="PS1"/>
                                        <ComboBoxItem Content="PS2"/>
                                        <ComboBoxItem Content="PS3"/>
                                        <ComboBoxItem Content="PS4"/>
                                        <ComboBoxItem Content="Alle"/>
                                    </ComboBox>
                                </DockPanel>
                            </DockPanel>
                            <UniformGrid Columns="3" Margin="3,0,3,0">
                                <DockPanel>
                                    <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Center" ><Run Text="Blattschnitt:"/></TextBlock>
                                    <Image ToolTip="Die Tabelle wird erst durch Blattschnitt gefiltert, wenn die Eingabe korrekt formatiert ist (z.B. 1122DB)." HorizontalAlignment="Left" Source="/resources/info.png" Stretch="Fill" Width="12" VerticalAlignment="Top"/>
                                </DockPanel>
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock TextWrapping="Wrap" Text="von" Margin="0,0,7,0"/>
                                    <TextBox x:Name="SectionInputFrom" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" MaxLength="6" TextChanged="SectionInputFrom_TextChanged"/>
                                </DockPanel>
                                <DockPanel LastChildFill="False" VerticalAlignment="Center" HorizontalAlignment="Right">
                                    <TextBlock TextWrapping="Wrap" Text="bis" Margin="0,0,7,0"/>
                                    <TextBox x:Name="SectionInputTo" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" TextChanged="SectionInputTo_TextChanged" MaxLength="6"/>
                                </DockPanel>
                            </UniformGrid>
                            <Grid Margin="3,0,3,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="91*"/>
                                    <ColumnDefinition Width="92*"/>
                                </Grid.ColumnDefinitions>
                                <DockPanel>
                                    <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Center" Text="Punktnr.:"/>
                                    <Image ToolTip="Die Tabelle wird durch Punktnr. gefiltert, indem Sie die OK-Taste drücken." HorizontalAlignment="Left" Source="/resources/info.png" Stretch="Fill" Width="12" VerticalAlignment="Top"/>
                                </DockPanel>
                                <DockPanel Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,0">
                                    <TextBlock TextWrapping="Wrap" Text="von" Margin="0,0,7,0"/>
                                    <TextBox x:Name="NumberInputFrom" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" MaxLength="5"/>
                                </DockPanel>
                                <DockPanel Grid.Column="2" LastChildFill="False" VerticalAlignment="Center" HorizontalAlignment="Right">
                                    <TextBlock TextWrapping="Wrap" Text="bis" Margin="0,0,7,0"/>
                                    <TextBox x:Name="NumberInputTo" TextWrapping="Wrap" DockPanel.Dock="Left" MinWidth="50" MaxLength="5" VerticalAlignment="Center"/>
                                    <Button x:Name="NumberFilterConfirmButton" DockPanel.Dock="Right" Content="OK" VerticalAlignment="Center" HorizontalAlignment="Center" Click="NumberFilterConfirmButton_Click" Width="20" Margin="10,0,0,0"/>
                                </DockPanel>
                            </Grid>
                            <DockPanel LastChildFill="False">
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="3,0,0,0">
                                    <TextBlock TextWrapping="Wrap" Text="Strecke:" Margin="0,0,7,0"/>
                                    <TextBox x:Name="LineInput" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" TextChanged="LineInput_TextChanged" IsEnabled="False"/>
                                </DockPanel>
                                <DockPanel HorizontalAlignment="Right" VerticalAlignment="Center" DockPanel.Dock="Right" Margin="0,0,3,0">
                                    <TextBlock TextWrapping="Wrap" Text="Auftrag:" Margin="0,0,7,0"/>
                                    <TextBox x:Name="ContractInput" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" HorizontalAlignment="Right" VerticalAlignment="Center" TextChanged="ContractInput_TextChanged"/>
                                </DockPanel>
                            </DockPanel>
                            <UniformGrid Columns="3" Margin="3,0,3,0">
                                <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,0,14,0"><Run Text="Kilometer:"/></TextBlock>
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock TextWrapping="Wrap" Text="von" Margin="0,0,7,0"/>
                                    <TextBox x:Name="KmInputFrom" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" TextChanged="KmInputFrom_TextChanged" IsEnabled="False"/>
                                </DockPanel>
                                <DockPanel LastChildFill="False" VerticalAlignment="Center" HorizontalAlignment="Right">
                                    <TextBlock TextWrapping="Wrap" Text="bis" Margin="0,0,7,0"/>
                                    <TextBox x:Name="KmInputTo" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="50" TextChanged="KmInputTo_TextChanged" IsEnabled="False"/>
                                </DockPanel>
                            </UniformGrid>
                            <Button x:Name="FilterResetButton" Content="Zurücksetzen" Click="FilterResetButton_Click" Width="74" HorizontalAlignment="Center"/>
                        </UniformGrid>
                    </Expander>
                    <GridSplitter HorizontalAlignment="Center" Grid.Column="2" Grid.Row="0"
                                  Grid.RowSpan="2" Width="5"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="EditTabItem" Header="Bearbeitung" Height="20" VerticalAlignment="Center">
                <!--MouseLeftButtonUp="EditTabItem_MouseLeftButtonUp"-->
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <DockPanel x:Name="TableForEditingPanel" Grid.Row="0" Grid.Column="0" Margin="5">
                        <DockPanel DockPanel.Dock="Top">
                            <ComboBox x:Name="TablesComboBox" DockPanel.Dock="Left" Height="22" HorizontalAlignment="Left" DropDownClosed="TablesComboBox_DropDownClosed">
                                <ComboBoxItem Content="Tabelle PP" IsSelected="True"/>
                                <ComboBoxItem x:Name="PhTableCbxItem" Content="Tabelle PH"/>
                                <ComboBoxItem x:Name="PkTableCbxItem" Content="Tabelle PK"/>
                                <ComboBoxItem x:Name="PlTableCbxItem" Content="Tabelle PL"/>
                                <ComboBoxItem x:Name="PsTableCbxItem" Content="Tabelle PS"/>
                            </ComboBox>
                            <Button x:Name="EntryRemoveButton" DockPanel.Dock="Right" HorizontalAlignment="Right" Content="Datensatz aus Bearbeitung entfernen" Click="EntryRemoveButton_Click"/>
                            <Button Content="Tabelle leeren" HorizontalAlignment="Right" Click="ClearButton_Click" DockPanel.Dock="Right" Margin="0,0,20,0"/>
                        </DockPanel>
                        <DataGrid x:Name="TableForEditing" DockPanel.Dock="Top" ColumnWidth="*" IsReadOnly="True"
                                  SelectionChanged="TableForEditing_SelectionChanged" AutoGeneratingColumn="TableForEditing_AutoGeneratingColumn"/>
                    </DockPanel>
                    <Grid x:Name="DataProcessingGrid" Grid.Column="1" Margin="5">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Border BorderBrush="Black" BorderThickness="1" Margin="0,0,0,5">
                            <Grid x:Name="PadEditGrid" Margin="5,0,5,5">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition MinHeight="20"/>
                                    <RowDefinition MinHeight="20"/>
                                    <RowDefinition MinHeight="20"/>
                                    <RowDefinition MinHeight="20"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="PAD ändern" SnapsToDevicePixels="True" HorizontalAlignment="Center" FontWeight="Bold"/>
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1">
                                    <TextBlock TextWrapping="Wrap" Text="aktueller PAD" Margin="0,0,7,0" FontWeight="Normal"/>
                                    <TextBox x:Name="CurPadInput" TextWrapping="Wrap" DockPanel.Dock="Right" HorizontalAlignment="Right" MinWidth="70" IsEnabled="False"/>
                                </DockPanel>
                                <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2" Margin="15,0,0,0">
                                    <TextBlock TextWrapping="Wrap" Text="neuer PAD" Margin="0,0,7,0" FontWeight="Normal"/>
                                    <TextBox x:Name="NewPadInput" TextWrapping="Wrap" DockPanel.Dock="Right" MinWidth="70"/>
                                </DockPanel>
                                <Grid x:Name="PadEditCheckBoxRow" Grid.Row="3">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox x:Name="PhCheckBox" Content="PH" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=PhTableCbxItem, Path=Visibility}" Value="Collapsed">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox x:Name="PlCheckBox" Content="PL" HorizontalAlignment="Center" Grid.Column="2">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=PlTableCbxItem, Path=Visibility}" Value="Collapsed">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox x:Name="PsCheckBox" Content="PS" Grid.Column="3" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=PsTableCbxItem, Path=Visibility}" Value="Collapsed">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox x:Name="PkCheckBox" Content="PK" Grid.Column="1" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsEnabled" Value="true" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=PkTableCbxItem, Path=Visibility}" Value="Collapsed">
                                                        <Setter Property="IsEnabled" Value="false" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                </Grid>
                                <Button x:Name="PadEditButton" Grid.Row="4" Content="Bestätigen" HorizontalAlignment="Center" VerticalAlignment="Center" Click="PadEditButton_Click"></Button>
                            </Grid>
                        </Border>
                        <Border Grid.Row="2" BorderBrush="Black" BorderThickness="1" Margin="0,5,0,0">
                            <Grid x:Name="DeleteGrid" Margin="5,0,5,5">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="20"/>
                                </Grid.RowDefinitions>
                                <TextBlock Text="Löschen" SnapsToDevicePixels="True" HorizontalAlignment="Center" FontWeight="Bold"/>
                                <Grid x:Name="DelCheckBoxRow1" Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox x:Name="PhDelCheckBox" Content="PH" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsChecked" Value="false" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=TablesComboBox, Path=SelectedIndex}" Value="1">
                                                        <Setter Property="IsChecked" Value="true" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox x:Name="PsDelCheckBox" Content="PS" Grid.Column="3" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsChecked" Value="false" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=TablesComboBox, Path=SelectedIndex}" Value="4">
                                                        <Setter Property="IsChecked" Value="true" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox x:Name="PkDelCheckBox" Content="PK" Grid.Column="1" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsChecked" Value="false" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=TablesComboBox, Path=SelectedIndex}" Value="2">
                                                        <Setter Property="IsChecked" Value="true" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </CheckBox.Style>
                                    </CheckBox>
                                    <CheckBox Grid.Column="2" x:Name="PlDelCheckBox" Content="PL" HorizontalAlignment="Center">
                                        <CheckBox.Style>
                                            <Style TargetType="CheckBox">
                                                <Setter Property="IsChecked" Value="false" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=TablesComboBox, Path=SelectedIndex}" Value="3">
